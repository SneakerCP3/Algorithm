# 二分查找
def binary_search(self, nums: List[int], target: int) -> int:
        left = 0
        right = len(nums) - 1             # 注意是len(nums)-1
        
        while left <= right:
            mid = (left + right)//2       # 要放在循环内
            if nums[mid] == target:
                return mid
            elif nums[mid] > target:
                right = mid -1
            else:
                left = mid + 1
        else:                             # 这个else得有，查找不到的时候返回-1
            return  -1
